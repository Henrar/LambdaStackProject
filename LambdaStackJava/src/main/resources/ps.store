I_HASHTAG: insert into TBL_HASHTAG(FLD_NAME) values(?);
FIND_HASHTAG: select FLD__ID from TBL_HASHTAG where FLD_NAME = ?;
I_KEYWORD_CATEGORY: insert into TBL_KEYWORD_CATEGORY(FLD_NAME) values(?);
I_KEYWORD: insert into TBL_KEYWORD_CATEGORY(FLD_NAME,FKF_CATEGORY) values(?,?);
LIST_KEYWORDS: select FLD_NAME from TBL_KEYWORD;